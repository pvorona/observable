{
    "compilerOptions": {
      "baseUrl": "./src",
      "paths": {
        "src/*": ["./src/*"]
      },
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es6",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "sourceMap": true,
    }
  }